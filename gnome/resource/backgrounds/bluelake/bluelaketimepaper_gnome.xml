<background>
  <starttime>
    <year>2019</year>
    <month>05</month>
    <day>07</day>
    <hour>07</hour>
    <minute>00</minute>
    <second>00</second>
  </starttime>
<!-- This animation will start at 7 AM. -->

<!-- We start with dawn at 7 AM. It will remain up for 2 hours. -->
<static>
<duration>7200.0</duration>
<file>/usr/share/backgrounds/gnome/bluelake_evening.jpg</file>
</static>

<!-- Dawn ended and starts to transition to day at 9 AM. The transition lasts for 2 hours, ending at 11 AM. -->
<transition type="overlay">
<duration>7200.0</duration>
<from>/usr/share/backgrounds/gnome/bluelake_evening.jpg</from>
<to>/usr/share/backgrounds/gnome/bluelake_morning.jpg</to>
</transition>

<!-- It's 11 AM, we're showing the day image in full force now, for 4 hours ending at 3 PM. -->
<static>
<duration>14400.0</duration>
<file>/usr/share/backgrounds/gnome/bluelake_morning.jpg</file>
</static>

<!-- It's 3 PM, and we're starting to transition to sunset. Transition completes at 7 PM. -->
<transition type="overlay">
<duration>14400.0</duration>
<from>/usr/share/backgrounds/gnome/bluelake_morning.jpg</from>
<to>/usr/share/backgrounds/gnome/bluelake_evening.jpg</to>
</transition>

<!-- It's 7 PM, we're showing the sunset for 2 hours ending at 9 PM. -->
<static>
<duration>7200.0</duration>
<file>/usr/share/backgrounds/gnome/bluelake_evening.jpg</file>
</static>

<!-- It's 9 PM, and we're starting to transition to twilight. Transition completes at 11 PM. -->
<transition type="overlay">
<duration>7200.0</duration>
<from>/usr/share/backgrounds/gnome/bluelake_evening.jpg</from>
<to>/usr/share/backgrounds/gnome/bluelake_night.jpg</to>
</transition>

<!-- It's 11 PM, we're showing the twilight till 5 AM. -->
<static>
<duration>21600.0</duration>
<file>/usr/share/backgrounds/gnome/bluelake_night.jpg</file>
</static>

<!-- It's 5 PM, and we're starting to transition to dawn. Transition completes at 7 AM. -->
<transition type="overlay">
<duration>7200.0</duration>
<from>/usr/share/backgrounds/gnome/bluelake_night.jpg</from>
<to>/usr/share/backgrounds/gnome/bluelake_evening.jpg</to>
</transition>

</background>
